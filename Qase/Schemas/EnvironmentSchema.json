{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "status",
    "result"
  ],
  "properties": {
    "status": {
      "type": "boolean",
      "default": false,
      "title": "The status Schema",
      "examples": [
        true
      ]
    },
    "result": {
      "type": "object",
      "default": {},
      "title": "The result Schema",
      "required": [
        "id",
        "title",
        "description",
        "slug",
        "host"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "default": 0,
          "title": "The id Schema",
          "examples": [
            1
          ]
        },
        "title": {
          "type": "string",
          "default": "",
          "title": "The title Schema",
          "examples": [
            "Implementation"
          ]
        },
        "description": {
          "type": "string",
          "default": "",
          "title": "The description Schema",
          "examples": [
            "Dicta libero numquam repellendus quia labore."
          ]
        },
        "slug": {
          "type": "string",
          "default": "",
          "title": "The slug Schema",
          "examples": [
            "Johnson - Schaden"
          ]
        },
        "host": {
          "type": "string",
          "default": "",
          "title": "The host Schema",
          "examples": [
            "Computers"
          ]
        }
      },
      "examples": [{
        "id": 1,
        "title": "Implementation",
        "description": "Dicta libero numquam repellendus quia labore.",
        "slug": "Johnson - Schaden",
        "host": "Computers"
      }]
    }
  },
  "examples": [{
    "status": true,
    "result": {
      "id": 1,
      "title": "Implementation",
      "description": "Dicta libero numquam repellendus quia labore.",
      "slug": "Johnson - Schaden",
      "host": "Computers"
    }
  }]
}